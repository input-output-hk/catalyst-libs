{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Catalyst signed document metadata schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "type": {
            "type": "string",
            "format": "uuid"
        },
        "id": {
            "type": "string",
            "format": "ulid"
        },
        "ver": {
            "type": "string",
            "format": "ulid"
        },
        "ref": {
            "type": "string",
            "format": "ulid"
        },
        "template": {
            "type": "string",
            "format": "ulid"
        },
        "reply": {
            "type": "string",
            "format": "ulid"
        },
        "section": {
            "type": "string"
        }
    },
    "required": [
        "type",
        "id",
        "ver"
    ]
}